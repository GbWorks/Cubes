{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Debug": {
      "IncludeScopes": true
    }
  },
  "AllowedHosts": "*",
  "Host": {
    "URLs": "http://localhost:3001",
    "UseSSL": false,
    "EnableCompression": false,
    "SwaggerTheme": "flattop",
    "WrapPath": "/api/",
    "WrapPathExclude": "/api/system/ping",
    "CorsPolicies": [
      {
        "PolicyName": "Management",
        "Origins": [
          "http://localhost:4200",
          "http://localhost:4201"
        ]
      }
    ]
  }
}